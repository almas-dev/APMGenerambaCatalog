import UIKit

class {{ module_info.name }}Configurator: Configurable {

    var flowController: FlowControllerProtocol?

    func configureModule() -> UIViewController {
        let router = {{ module_info.name }}Router()
        router.flowController = flowController

        let presenter = {{ module_info.name }}Presenter()
        presenter.router = router

        let interactor = {{ module_info.name }}Interactor()
        interactor.output = presenter

        let viewController = {{ module_info.name }}ViewController(output: presenter)

        presenter.interactor = interactor
        presenter.view = viewController

        return viewController
    }
}
